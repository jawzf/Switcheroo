{
    "name": "switcheroo",
    "version": "0.1.1",
    "description": "Import Google Play Music Library to Spotify!",
    "permissions": ["declarativeContent", "storage", "tabs",
        "http://*/",
        "https://*/",
        "identity"
    ],
    "background": {
        "scripts": ["background.js", "jquery-3.3.1.min.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/googlify16.png",
            "32": "images/googlify32.png",
            "64": "images/googlify64.png",
            "128": "images/googlify128.png"
        }
    },
    "icons": {
        "16": "images/googlify16.png",
        "32": "images/googlify32.png",
        "64": "images/googlify64.png",
        "128": "images/googlify128.png"
    },
    "manifest_version": 2
}